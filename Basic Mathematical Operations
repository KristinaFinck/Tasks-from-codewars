function basicOp(operation, value1, value2){
  //Code
  let result;
  
  swithc(operation) {
    case '+':
    result = value1 + value2;
    break;
    case '-':
    result = value1 - value2;
    break;
    case '*':
    result = value1 * value2;
    break;
    case '/':
    result = value1 / value2;
    break;
  }
  return result
}
 console.log(basicOp(-, 10, 4))
